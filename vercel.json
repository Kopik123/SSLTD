{
  "version": 2,
  "name": "ssltd",
  "builds": [
    {
      "src": "api/**/*.php",
      "use": "vercel-php@0.6.0"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|svg|gif|webp|ico))",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=604800, immutable"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "0"
  },
  "regions": ["lhr1"]
}
