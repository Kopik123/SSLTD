# Staging Environment Configuration
# Copy this file to .env.staging for staging deployments

APP_ENV=staging
APP_DEBUG=0
APP_URL=https://your-staging-domain.com
APP_KEY=change-me-to-random-string-32chars

# Staging Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=ss_ltd_staging
DB_USER=staging_user
DB_PASS=your-staging-password

SERVICE_AREA_RADIUS_MILES=60

# Dev tools disabled in staging
SS_DEV_TOOLS_KEY=
