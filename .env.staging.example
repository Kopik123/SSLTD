APP_ENV=prod
APP_DEBUG=0
APP_URL=http://127.0.0.1:8001

# NOTE: replace with a strong random value (32+ chars). In prod-like env the app
# will refuse to start if APP_KEY is missing/weak.
APP_KEY=change-me-staging-0123456789abcdef0123456789abcdef

# mysql|sqlite
DB_CONNECTION=mysql

# Used only when DB_CONNECTION=sqlite
DB_DATABASE=storage/app_staging.db
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=ss_ltd_staging
DB_USER=root
DB_PASS=

SERVICE_AREA_RADIUS_MILES=60

